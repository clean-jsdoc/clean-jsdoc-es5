{
    "globals": {
        "Alive": "readonly",
        "Environment": "readonly",
        "Energy": "readonly",
        "TAFFY": "readonly",
        "Tutorial": "readonly"
    },
    "extends": [
        "plugin:jsdoc/recommended"
    ],
    "rules": {
        "class-methods-use-this": "off",
        "no-empty-function": "off",
        "no-invalid-this": "off",
        "jsdoc/require-param-description": "off",
        "jsdoc/require-yields-check":"off"
    }
}
