{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./demo/src", "./README.md"]
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "idInHeadings": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "demo/output",
        "recurse": true,
        "template": "./",
        "tutorials": "./demo/src/tutorials",
        "theme_opts": {
            "theme": "dynamic",
            "langNames": false,
            "menuLocation": "down",
            "project": {
                "title": "clean-jsdoc",
                "version": "4.6.1",
                "repo": "https://github.com/clean-jsdoc/clean-jsdoc-es5"
            },
            "sort": false,
            "sections": [
                "modules",
                "classes",
                "events",
                "global",
                "tutorials"
            ],
            "menu": [
                {
                    "title": "Blog",
                    "link": "https://rdipardo.bitbucket.io"
                },
                {
                    "title": "Other Projects",
                    "link": "https://bitbucket.org/rdipardo/workspace/repositories",
                    "target": "_blank"
                }
            ],
            "meta": [
                {
                    "name": "author",
                    "content": "Robert Di Pardo"
                },
                {
                    "name": "description",
                    "content": "ES5 compatible fork of clean-jsdoc-theme"
                },
                {
                    "name": "attribution",
                    "content": "Favicon made by https://www.freepik.com from https://www.flaticon.com"
                }
            ],
            "asset_paths" : [ "img", "../does/not/exist" ],
            "remote_assets": [
                {
                    "href": "https://raw.githubusercontent.com/clean-jsdoc/clean-jsdoc-es5/c66f2cb8e2592b7a0185bf2e6160bcdd698063dd/img/html-5.png",
                    "type": "image/x-icon"
                }
            ],
            "overlay_scrollbar": {
                "options": {
                    "autoUpdate": false
                }
            },
            "inline_style": ".readme img { max-width: 900px; }",
            "codepen": {
                "options": {
                    "editors": "001",
                    "js_pre_processor": "babel"
                }
            }
        }
    }
}
